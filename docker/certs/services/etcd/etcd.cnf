[req]
default_bits = 2048
prompt = no
default_md = sha256
distinguished_name = dn
req_extensions = req_ext

[dn]
C = US
ST = California
L = San Francisco
O = ThaliumX
OU = etcd
CN = etcd

[req_ext]
subjectAltName = @alt_names

[alt_names]
DNS.1 = etcd
DNS.2 = etcd-1
DNS.3 = etcd-2
DNS.4 = etcd-3
DNS.5 = localhost
IP.1 = 127.0.0.1
